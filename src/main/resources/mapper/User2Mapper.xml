<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.backend.mapper.User2Mapper">

<select id="getList" resultType="com.example.backend.dto.User2Dto">
	select * from user2
</select>

<insert id="insertUser2">
	insert into user2 (id, name, company) values (#{id}, #{name}, #{company})
</insert>

<delete id="deleteUser2" parameterType="com.example.backend.dto.User2Dto">
	delete from user2 where id = #{id}
</delete>

<update id="updateUser2" parameterType="com.example.backend.dto.User2Dto">
	update user2 set name=#{name}, company=#{company}
</update>

<select id="getSearchUser2" parameterType="INTEGER" resultType="com.example.backend.dto.User2Dto">
	select name, company from user2
</select>

</mapper>